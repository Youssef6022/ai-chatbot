ption expired, show modal\n    if (!isSubscribedToUser || (subscriptionDetails && new Date(subscriptionDetails.expires_at) < new Date())) {\n      setShowSubscriptionModal(true)\n    }\n  }\n\n  const processSubscription = async () => {\n    if (!profile) return\n\n    const result = await subscribeToCreator(profile.id)\n\n    if (result.success) {\n      setIsSubscribedToUser(true)\n\n      // Recharger les dÃ©tails de l'abonnement\n      const subDetails = await getSubscriptionDetails(profile.id)\n      if (subDetails.success && subDetails.data) {\n        setSubscriptionDetails(subDetails.data)\n      }\n    } else {\n      // Show error message to user\n      alert(`âŒ ${result.error || 'Subscription failed'}`)\n      throw new Error(result.error || 'Subscription failed')\n    }\n  }\n\n  const handleLikePost = async (postId: string) => {\n    if (!user) {\n      setAuthMode('signup')\n      setShowAuthModal(true)\n      return\n    }\n\n    const isLiked = likedPosts.has(postId)\n    const newLikedPosts = new Set(likedPosts)\n\n    if (isLiked) {\n      newLikedPosts.delete(postId)\n      await unlikePost(postId)\n    } else {\n      newLikedPosts.add(postId)\n      await likePost(postId)\n    }\n    setLikedPosts(newLikedPosts)\n\n    setPosts(posts.map(post => {\n      if (post.id === postId) {\n        return {\n          ...post,\n          likes_count: isLiked ? (post.likes_count || 0) - 1 : (post.likes_count || 0) + 1\n        }\n      }\n      return post\n    }))\n  }\n\n  const formatNumber = (num: number) => {\n    if (num >= 1000) {\n      return (num / 1000).toFixed(1) + 'K'\n    }\n    return num.toString()\n  }\n\n  const getSubscriptionButtonText = () => {\n    // Si on a les dÃ©tails de l'abonnement, les utiliser\n    if (subscriptionDetails) {\n      const expiresAt = new Date(subscriptionDetails.expires_at)\n      const now = new Date()\n      const isExpired = expiresAt < now\n\n      if (!isExpired && subscriptionDetails.status === 'active')