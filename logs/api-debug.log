hModal(true)\n      return\n    }\n\n    try {\n      console.log('[Profile] Creating conversation with:', profile.id)\n\n      // Create or get conversation\n      const result = await getOrCreateConversation(profile.id)\n\n      if (result.success && result.data) {\n        console.log('[Profile] Conversation created/retrieved:', result.data)\n\n        // Store conversation ID in localStorage\n        localStorage.setItem('openConversation', result.data)\n\n        // Small delay to ensure DB is updated\n        await new Promise(resolve => setTimeout(resolve, 100))\n\n        // Redirect to messages page\n        router.push('/messages')\n      } else {\n        console.error('[Profile] Failed to create conversation:', result.error)\n      }\n    } catch (error) {\n      console.error('[Profile] Error creating conversation:', error)\n    }\n  }\n\n  if (loading) {\n    return <LoadingScreen message=\"Chargement du profil...\" />\n  }\n\n  if (!profile) {\n    return (\n      <div className=\"min-h-screen bg-[var(--bg-base)] flex items-center justify-center\">\n        <div className=\"flex flex-col items-center gap-4\">\n          <span className=\"material-symbols-outlined text-6xl text-[var(--text-tertiary)]\">person_off</span>\n          <div className=\"text-center\">\n            <h2 className=\"text-xl font-bold text-[var(--text-primary)] mb-2\">Profil introuvable</h2>\n            <p className=\"text-[var(--text-secondary)]\">Ce profil n&apos;existe pas ou a Ã©tÃ© supprimÃ©</p>\n          </div>\n        </div>\n      </div>\n    )\n  }\n\n  const mediaPosts = posts.filter(p => p.media_urls && p.media_urls.length > 0)\n\n  return (\n    <div className=\"min-h-screen bg-[var(--bg-base)] flex overflow-x-hidden\">\n      {/* Sidebar - Show different version based on authentication */}\n      {currentUser ? (\n        <ModernSidebar username={currentUser?.username} onExpandChange={setSidebarExpanded} />\n      ) : (\n        <UnauthenticatedSidebar onExpandChange={setSidebarExpanded} />\n      )}\n\n      {/* Main Layout - Properly centered */}\n      <div className={`flex-1 flex justify-center transition-[padding] duration-700 ease-[cubic-bezier(0.25,0.1,0.25,1)] ${\n        sidebarExpanded ? 'md:pl-[300px]' : 'md:pl-[92px]'\n      } lg:pr-[340px] pl-0 pr-0 overflow-x-hidden`}>\n        {/* Main Content */}\n        <main className=\"w-full md:max-w-[600px] lg:max-w-[700px] md:border-l md:border-r border-[var(--border)] bg-[var(--surface)] overflow-x-hidden box-border\">\n          {/* Cover & Avatar */}\n          <div className=\"relative\">\n            <div\n              className=\"h-48 bg-cover bg-center cursor-pointer relative group\"\n              onClick={(e) => {\n                if (!(e.target as HTMLElement).closest('button')) {\n                  profile.cover_image_url && setViewerImage(profile.cover_image_url)\n                }\n              }}\n              style={{\n                backgroundImage: profile.cover_image_url\n                  ? `radial-gradient(circle, rgba(0,0,0,0.2) 0%, rgba(0,0,0,0.8) 100%), url('${profile.cover_image_url}')`\n                  : 'radial-gradient(circle, rgba(0,0,0,0.2) 0%, rgba(0,0,0,0.8) 100%), linear-gradient(135deg, var(--card), var(--surface))'\n              }}\n            >\n            </div>\n            <div className=\"absolute -bottom-12 left-4 md:left-6\">\n              <div\n                className=\"h-20 w-20 md:h-24 md:w-24 rounded-2xl border-4 border-[var(--surface)] bg-cover bg-center cursor-pointer hover:opacity-90 transition-opacity\"\n                onClick={() => setViewerImage(profile.avatar_url || DEFAULT_AVATAR_URL)}\n                style={{ backgroundImage: `url('${profile.avatar_url || DEFAULT_AVATAR_URL}')` }}\n              />\n            </div>\n          </div>\n\n          {/* Profile Info */}\n          <div className=\"px-4 md:px-6 pt-16 pb-4\">\n            <div className=\"flex flex-col md:flex-row md:items-start md:justify-between gap-4\">\n              <div className=\"flex-1\">\n                <div className=\"flex items-center gap-2\">\n                  <h2 className=\"text-2xl font-bold text-[var(--text-primary)]\">\n                    {profile.display_name || profile.username}\n                  </h2>\n                  {profile.is_verified && (\n                    <div className=\"relative group inline-flex items-center\">\n                      <span className=\"material-symbols-outlined text-lg text-[var(--accent)] cursor-help\">verified</span>\n                      <div className=\"absolute bottom-full left-1/2 transform -translate-x-1/2 mb-2 px-2 py-1 bg-black/90 text-white text-xs rounded-md whitespace-nowrap opacity-0 invisible group-hover:opacity-100 group-hover:visible transition-all duration-200 pointer-events-none z-50\">\n                        <div className=\"font-medium\">Verified Account</div>\n                        <div className=\"text-[10px] opacity-80\">This user has been verified by OnlyFreakys</div>\n                        <div className=\"absolute top-full left-1/2 transform -translate-x-1/2 -mt-1\">\n                          <div className=\"border-4 border-transparent border-t-black/90\"></div>\n                        </div>\n                      </div>\n                    </div>\n                  )}\n                  {profile.is_online && (\n                    <div className=\"flex items-center gap-1\">\n                      <div className=\"w-1.5 h-1.5 bg-green-500 rounded-full\"></div>\n                      <span className=\"text-xs text-green-600 dark:text-green-400\">En ligne</span>\n                    </div>\n                  )}\n                </div>\n                <p className=\"text-[var(--text-secondary)]\">\n                  @{profile.username} {profile.location && `Â· ${profile.location}`}\n                </p>\n                {profile.bio && (\n                  <p className=\"mt-2 text-[var(--text-primary)]\">{profile.bio}</p>\n                )}\n                <div className=\"mt-2 flex items-center gap-4 text-sm text-[var(--text-secondary)]\">\n                  <span>\n                    <span className=\"font-bold text-[var(--text-primary)]\">\n                      {followersCount !== null ? formatNumber(followersCount) : '...'}\n                    </span> {t.profile.followers}\n                  </span>\n                  <span>\n                    <span className=\"font-bold text-[var(--text-primary)]\">\n                      {followingCount !== null ? formatNumber(followingCount) : '...'}\n                    </span> {t.profile.following}\n                  </span>\n                  <span>\n                    <span className=\"font-bold text-[var(--text-primary)]\">\n                      {formatNumber(posts.length)}\n                    </span> {t.profile.posts}\n                  </span>\n                </div>\n\n              </div>\n\n              {/* Action Buttons */}\n              {currentUser && currentUser.username !== profile.username ? (\n                <div className=\"flex gap-3 w-full md:w-auto\">\n                  <button\n                    onClick={handleFollow}\n                    className={`group h-10 rounded-2xl px-7 text-sm font-semibold transition-all duration-200 cursor-pointer hover:scale-[1.02] flex-1 md:flex-none ${\n                      isFollowingUser\n                        ? 'bg-[var(--surface)] text-[var(--text-primary)] border-2 border-[var(--border)] hover:border-[var(--text-secondary)]'\n                        : 'bg-[var(--card)] text-[var(--text-primary)] border-2 border-[var(--border)] hover:bg-[var(--border)]'\n                    }`}\n                  >\n                    <div className=\"flex items-center gap-2 justify-center\">\n                      {isFollowingUser ? (\n                        <>\n                          <span className=\"material-symbols-outlined text-base\">check_circle</span>\n                          <span>{t.profile.followingStatus}</span>\n                        </>\n                      ) : (\n                        <>\n                          <span className=\"material-symbols-outlined text-base\">person_add</span>\n                          <span>{t.profile.follow}</span>\n                        </>\n                      )}\n                    </div>\n                  </button>\n                  <button\n                    onClick={handleMessage}\n                    className=\"group h-10 w-10 rounded-2xl border-2 border-[var(--border)] bg-[var(--card)] text-[var(--text-primary)] hover:bg-[var(--border)] transition-all duration-200 cursor-pointer hover:scale-[1.02] flex items-center justify-center\"\n                  >\n                    <span className=\"material-symbols-outlined text-lg\">mail</span>\n                  </button>\n                </div>\n              ) : currentUser && currentUser.username === profile.username ? (\n                <div className=\"flex gap-3 w-full md:w-auto\">\n                  <Link\n                    href=\"/settings\"\n                    className=\"h-10 rounded-xl bg-[var(--card)] px-6 text-sm font-semibold text-[var(--text-primary)] transition-colors hover:bg-[var(--border)] flex items-center justify-center w-full md:w-auto\">\n                    <span className=\"material-symbols-outlined text-sm mr-1 align-middle\">edit</span>\n                    {t.profile.editProfile}\n                  </Link>\n                </div>\n              ) : !currentUser ? (\n                <div className=\"flex gap-3 w-full md:w-auto\">\n                  <button\n                    onClick={() => {\n                      setAuthMode('signup')\n                      setShowAuthModal(true)\n                    }}\n                    className=\"h-10 rounded-xl bg-[var(--accent)] px-6 text-sm font-semibold text-white transition-all duration-200 hover:opacity-90 w-full md:w-auto\">\n                    <span className=\"material-symbols-outlined text-sm mr-1 align-middle\">person_add</span>\n                    {t.profile.signInToFollow}\n                  </button>\n                </div>\n              ) : null}\n            </div>\n          </div>\n\n          {/* Subscribe Button/Badge - Centered on page */}\n          {currentUser && currentUser.username !== profile.username && (profile.subscription_price ?? 0) > 0 && (\n            <div className=\"w-full py-5 flex justify-center\">\n              {getSubscriptionButtonText().isActive ? (\n                // Badge Ã©lÃ©gant pour les abonnÃ©s\n                <div className=\"flex flex-col items-center gap-2\">\n                  <div className=\"flex items-center gap-2 px-4 py-2 bg-gradient-to-r from-[var(--accent)]/10 to-[var(--accent)]/5 rounded-full border border-[var(--accent)]/30\">\n                    <span className=\"material-symbols-outlined text-lg text-[var(--accent)]\">\n                      workspace_premium\n                    </span>\n                    <span className=\"text-sm font-semibold text-[var(--text-primary)]\">\n                      AbonnÃ© actif\n                    </span>\n                  </div>\n                  {getSubscriptionButtonText().subText && (\n                    <span className=\"text-xs text-[var(--text-secondary)] opacity-80\">\n                      {getSubscriptionButtonText().subText}\n                    </span>\n                  )}\n                </div>\n              ) : (\n                // Bouton pour s'abonner\n                <button\n                  onClick={handleSubscribe}\n                  className=\"group relative rounded-xl px-6 py-2 text-sm font-semibold transition-all duration-200 cursor-pointer hover:scale-[1.02] bg-[var(--accent)] text-white border-2 border-[var(--accent)] hover:bg-[var(--accent)]/90 shadow-lg shadow-[var(--accent)]/20\"\n                >\n                  <div className=\"flex items-center gap-2\">\n                    <span className=\"material-symbols-outlined text-base\">\n                      {getSubscriptionButtonText().icon}\n                    </span>\n                    <span>{getSubscriptionButtonText().text}</span>\n                  </div>\n                </button>\n              )}\n            </div>\n          )}\n\n          {/* Tabs */}\n          <div className=\"sticky top-0 z-10 border-b border-[var(--border)] bg-[var(--surface)]\">\n            <nav className=\"-mb-px flex space-x-4 md:space-x-6 px-4 md:px-6 overflow-x-auto\">\n              <button\n                onClick={() => setActiveTab('posts')}\n                className={`relative shrink-0 whitespace-nowrap border-b-2 px-1 py-3 text-sm font-semibold ${\n                  activeTab === 'posts'\n                    ? 'border-[var(--accent)] text-[var(--text-primary)]'\n                    : 'border-transparent text-[var(--text-secondary)] hover:border-[var(--text-secondary)] hover:text-[var(--text-primary)]'\n                }`}\n              >\n                {t.profile.posts} <span className=\"ml-1 rounded-md bg-[var(--card)] px-1.5 py-0.5 text-xs text-[var(--text-secondary)]\">\n                  {formatNumber(posts.length)}\n                </span>\n              </button>\n              <button\n                onClick={() => setActiveTab('media')}\n                className={`shrink-0 whitespace-nowrap border-b-2 px-1 py-3 text-sm font-semibold ${\n                  activeTab === 'media'\n                    ? 'border-[var(--accent)] text-[var(--text-primary)]'\n                    : 'border-transparent text-[var(--text-secondary)] hover:border-[var(--text-secondary)] hover:text-[var(--text-primary)]'\n                }`}\n              >\n                {t.profile.media} <span className=\"ml-1 rounded-md bg-[var(--card)] px-1.5 py-0.5 text-xs text-[var(--text-secondary)]\">\n                  {formatNumber(mediaPosts.length)}\n                </span>\n              </button>\n              <button\n                onClick={() => setActiveTab('shop')}\n                className={`shrink-0 whitespace-nowrap border-b-2 px-1 py-3 text-sm font-semibold ${\n                  activeTab === 'shop'\n                    ? 'border-[var(--accent)] text-[var(--text-primary)]'\n                    : 'border-transparent text-[var(--text-secondary)] hover:border-[var(--text-secondary)] hover:text-[var(--text-primary)]'\n                }`}\n              >\n                {t.profile.shop} <span className=\"ml-1 rounded-md bg-[var(--card)] px-1.5 py-0.5 text-xs text-[var(--text-secondary)]\">\n                  0\n                </span>\n              </button>\n            </nav>\n          </div>\n\n          {/* Content based on active tab */}\n          {activeTab === 'posts' && (\n            <div className=\"divide-y divide-[var(--border)]\">\n              {/* Create Post Card - Only show on own profile */}\n              {currentUser && currentUser.username === profile.username && (\n                <CreatePostCard\n                  onPostCreated={() => {\n                    loadProfile()\n                    router.refresh()\n                  }}\n                  userAvatar={currentUser.avatar_url || undefined}\n                />\n              )}\n\n              {posts.length === 0 ? (\n                <div className=\"p-12 text-center\">\n                  <p className=\"text-[var(--text-secondary)]\">{t.profile.noPostsYet}</p>\n                </div>\n              ) : (\n                posts.map((post) => (\n                  <EnhancedPostCard\n                    key={post.id}\n                    post={{...post, profile}}\n                    isLiked={likedPosts.has(post.id)}\n                    isSubscribed={isSubscribedToUser}\n                    isFollowing={isFollowingUser}\n                    onLike={() => handleLikePost(post.id)}\n                    onFollow={handleFollow}\n                    onSubscribe={handleSubscribe}\n                    currentUserId={user?.id}\n                    currentUserAvatar={currentUser?.avatar_url || undefined}\n                    currentUserBalance={currentUser?.wallet || 0}\n                    onPaidContentPurchased={loadProfile}\n                  />\n                ))\n              )}\n            </div>\n          )}\n\n          {activeTab === 'media' && (\n            <div className=\"px-4 md:px-6 py-4\">\n              {/* Media Filters */}\n              <div className=\"flex items-center gap-2 md:gap-3 mb-6 overflow-x-auto pb-2\">\n                <button\n                  onClick={() => setMediaFilter('all')}\n                  className={`px-4 py-1.5 rounded-full transition-all whitespace-nowrap text-sm ${\n                    mediaFilter === 'all'\n                      ? 'bg-[var(--text-primary)] text-[var(--bg-base)] font-medium'\n                      : 'bg-transparent text-[var(--text-secondary)] border border-[var(--border)] hover:border-[var(--text-secondary)]'\n                  }`}\n                >\n                  Tout {mediaPosts.length}\n                </button>\n                <button\n                  onClick={() => setMediaFilter('photos')}\n                  className={`px-4 py-1.5 rounded-full transition-all whitespace-nowrap text-sm ${\n                    mediaFilter === 'photos'\n                      ? 'bg-[var(--text-primary)] text-[var(--bg-base)] font-medium'\n                      : 'bg-transparent text-[var(--text-secondary)] border border-[var(--border)] hover:border-[var(--text-secondary)]'\n                  }`}\n                >\n                  Photos {mediaPosts.filter(p => p.media_urls?.some(url => url.match(/\\.(jpg|jpeg|png|gif|webp)$/i))).length}\n                </button>\n                <button\n                  onClick={() => setMediaFilter('videos')}\n                  className={`px-4 py-1.5 rounded-full transition-all whitespace-nowrap text-sm ${\n                   